extends /resources/jade/list-checkable

block title
  | ISO Manager

block headings
  th.relative Name
    sort(col="name")
  th Status
  th.relative Uploaded
    sort(col="created_at")
  th Actions

block columns
  td {{ row.name }}
  td {{ row.status }}
  td {{ row.created_at }}
  td
    .btn-group.btn-group-xs
      a.btn.btn-default(ui-sref="app.pxe.iso.view({id: row.id})")
        | {{ 'action.EDIT' | translate }}
