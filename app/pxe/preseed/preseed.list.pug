extends /resources/pug/list-checkable

block title
 | Preseeds

block headings
  th Name
  th Description
  th Installable
  th Actions

block columns
  td {{ row.name }}
  td {{ row.description }} 
  td
    span.fa.fa-check.text-success(ng-if="row.is_installable")
  td
    .btn-group.btn-group-xs
      a.btn.btn-default(ui-sref="app.pxe.preseed.view({id: row.id})")
        | {{ 'app.action.EDIT' | translate }}
